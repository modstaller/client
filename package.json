{
  "name": "modstaller-client",
  "productName": "Modstaller",
  "version": "0.0.0",
  "main": "src/main.js",
  "private": true,
  "scripts": {
    "build-css": "tailwindcss build styles/styles.css -o public/styles.css",
    "build-react": "react-scripts build",
    "dev": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && npm run start-electron\"",
    "prepare": "npm run build-css",
    "start-electron": "electron .",
    "start-react": "react-scripts start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modstaller/client.git"
  },
  "bugs": {
    "url": "https://github.com/modstaller/client/issues"
  },
  "homepage": ".",
  "browserslist": {
    "//": "Use https://browserl.ist to test the list",
    "production": [
      "chrome >= 81"
    ],
    "development": [
      "chrome >= 81"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^9.0.0-beta.10",
    "electron-builder": "^22.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.2.0",
    "wait-on": "^4.0.1"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "fs-extra": "^9.0.0",
    "got": "^10.7.0"
  }
}
